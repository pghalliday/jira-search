{
  "name": "jira-search",
  "version": "0.1.0",
  "description": "Search jira",
  "scripts": {
    "lint": "coffeelint -f coffeelint.json -x node_modules .",
    "coverage": "multi='spec=- html-cov=coverage.html mocha-cov-reporter=-' mocha --recursive --compilers coffee:coffee-script/register --require coffee-coverage/register -R mocha-multi",
    "pretest": "npm run lint",
    "test": "npm run coverage"
  },
  "config": {
    "mocha-cov-reporter": {
      "failOnError": true,
      "threshold": 100,
      "useColors": true
    }
  },
  "keywords": [
    "jira",
    "search"
  ],
  "homepage": "https://github.com/pghalliday/jira-search",
  "bugs": {
    "url": "https://github.com/pghalliday/jira-search/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Peter Halliday",
    "email": "pghalliday@gmail.com",
    "url": "http://pghalliday.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pghalliday/jira-search.git"
  },
  "dependencies": {
    "JSONStream": "^0.10.0",
    "q": "^1.1.2",
    "request": "^2.51.0",
    "stream-reduce": "^1.0.3"
  },
  "devDependencies": {
    "chai": "^1.10.0",
    "coffee-script": "^1.8.0",
    "coffee-coverage": "^0.4.4",
    "coffeelint": "^1.8.1",
    "mocha": "^2.1.0",
    "mocha-cov-reporter": "^1.1.3",
    "mocha-multi": "^0.6.0",
    "sinon": "^1.12.2",
    "sinon-chai": "^2.6.0"
  }
}
